%Command for naming strokes

%Horizontal strokes
	%Top
\def\TopHorizontal{(UpperLeft) -- (UpperRight);}
\def\LeftTopHorizontal{(OuterUpperLeft) -- (CenterCeiling);}
\def\RightTopHorizontal{(OuterTopRight) -- (CenterCeiling);}
\def\LeftRightTopHorizontal{(CenterCeiling) -- (UpperRight);}
\def\RightLeftTopHorizontal{(CenterCeiling) -- UpperLeft);}	
	%Upper
\def\UpperHorizontal{(CenterUpperLeft) -- (CenterUpperRight);}
\def\LeftUpperHorizontal{(CenterUpperLeft) -- (CenterUpper);}
\def\RightUpperHorizontal{(CenterUpperRight) -- (CenterUpper);}
\def\OuterRightUpperHorizontal{(CenterUpperRight) -- (OuterUpperRight);}
\def\OuterLeftUpperHorizontal{(CenterUpperLeft)--(OuterUpperLeft);}
\def\QRightUpperHorizontal{(QUpperLeft) -- (CenterUpperRight);}
\def\QLeftUpperHorizontal{(QUpperRight) -- (CenterUpperLeft);}
	%Center
\def\CH{(CenterLeft) -- (CenterRight)}
\def\CenterHorizontal{(CenterLeft) -- (CenterRight);}
\def\LeftCenterHorizontal{(OuterCenterLeft) -- (CenterLeft);}
\def\RightCenterHorizontal{(Center) -- (CenterRight);}
%\def\RightCenterHorizontalRight{(}
	%Lower
\def\LowerHorizontal{(CenterBottomLeft) -- (CenterBottomRight);}
\def\LeftLowerHorizontal{(CenterBottomLeft) -- (CenterBottom);}
\def\RightLowerHorizontal{(CenterBottomRight) -- (CenterBottom);}
	%Bottom
\def\BottomHorizontal{(BottomLeft) -- (BottomRight);}
\def\LeftBottomHorizontal{(OuterBottomLeft) -- (CenterCellar);}
\def\RightLeftBottomHorizontal{(BottomLeft) -- (CenterCellar);}
\def\RightBottomHorizontal{(OuterBottomRight) -- (CenterCellar);}
\def\LeftRightBottomHorizontal{(BottomRight) -- (CenterCellar);}

	%Hell
	
\def\HellHorizontal{(LeftHell) -- (RightHell);}

%Vertical Strokes
\def\LV{(BottomLeft) -- (UpperLeft)}
\def\LeftVertical{(BottomLeft) -- (UpperLeft)}
\def\CenterVertical{(CenterCellar) -- (CenterCeiling);}
\def\RightVertical{(BottomRight) -- (UpperRight);}

\def\CenterUpperVertical{(CenterCellar) -- (CenterUpper);}
\def\CenterBottomVertical{(CenterCeiling) -- (CenterBottom);}

%Diagonal strokes - adjusted thickness!!!!!
\def\RightDiagonal{(BottomLeft) -- (UpperRight);}
\def\LeftDiagonal{(BottomRight) -- (UpperLeft);}
% - Diagonal strokes - hypothenuses; smaller diamond
\def\HypoBottomLeft{(CenterLeft) -- (CenterCellar);}
\def\HypoBottomRight{(CenterRight) -- (CenterCellar);}
\def\HypoUpperLeft{(CenterLeft) -- (CenterCeiling);}
\def\HypoUpperRight{(CenterRight) -- (CenterCeiling);}

\def\BigHypoUpperLeft{(CenterLeft)--(UpperRight);}
\def\BigHypoUpperRight{(CenterRight) -- (UpperLeft);}

\def\LeftBigHypoUpperLeft{(CenterLeft) -- (CenterUpper);}
\def\RightBigHypoUpperLeft{(CenterUpper) -- (UpperRight);}

\def\RightBigHypoUpperRight{(CenterRight) -- (CenterUpper);}
\def\LeftBigHypoUpperRight{(CenterUpper) -- (UpperLeft);}

\def\BigHypoCenterLeft{(CenterBottomLeft) -- (CenterUpperRight);}

\def\LeftBigHypoCenterLeft{(CenterBottomLeft) -- (Center);}
\def\LeftBigHypoCenterLeft{(Center) -- (CenterUpperRight);}

\def\BigHypoCenterRight{(CenterBottomRight) -- (CenterUpperLeft);}
\def\LeftBigHypoCenterRight{(CenterBottomRight) -- (Center);}
\def\RightBigHypoCenterRight{(Center) -- (CenterUpperLeft);}

\def\BigHypoBottomLeft{(BottomLeft) -- (CenterRight);}
\def\BigHypoBottomRight{(BottomRight) -- (CenterLeft);}

\def\LeftBigHypoBottomLeft{(BottomLeft) -- (CenterBottom);}
\def\RightBigHypoBottomLeft{(CenterBottom) -- (CenterRight);}

\def\RightBigHypoBottomRight{(BottomRight) -- (CenterBottom);}
\def\LeftBigHypoBottomRight{(CenterBottom) -- (CenterLeft);}


\def\BigHypoCenterLeft{(CenterBottomLeft) -- (CenterUpperRight);}
\def\BigHypoCenterRight{(CenterBottomRight) -- (CenterUpperLeft);}

\def\LeftHypoHeaven{(CenterCeiling) -- (LeftHeaven);}
\def\RightHypoHeaven{(CenterCeiling) -- (RightHeaven);}


\def\BigHypoHellLeft{(HellLeft) -- (CenterBottomRight);}
\def\LeftBigHypoHellLeft{(HellLeft) -- (CenterCellar);}
\def\RightBigHypoHellLeft{(CenterCellar) -- (CenterBottomRight);}

\def\BigHypoHellRight{(HellRight) -- (CenterBottomLeft);}
\def\LeftBigHypoHellRight{(HellRight) -- (CenterCellar);}
\def\RightBigHypoHellRight{(CenterCellar) -- (CenterBottomLeft);}

\def\BigLowerHypoHellLeft{(CenterHell) -- (OuterLowerRight);}
\def\LeftLowerHypoHellLeft{(BottomRight) -- (CenterHell);}
\def\RightLowerHypoHellLeft{(CenterHell) -- (OuterLowerRight);}


\def\RightUpperInnerDiagonal{(QUpperRight) -- (QCenterLeft);}
\def\RightBottomInnerDiagonal{(QCenterRight) -- (QBottomLeft);}

\def\LeftUpperInnerDiagonal{(QUpperLeft) -- (QCenterRight);}
\def\LeftBottomInnerDiagonal{(QCenterLeft) -- (QBottomRight);}


% - Diagonal strokes - From left to TopHorizontal
\def\RightCenterDiagonal{(BottomLeft) -- (CenterCeiling);}
\def\LeftCenterDiagonal{(BottomRight) -- (CenterCeiling);}

\def\LeftCenterUpperDiagonal{(BottomRight) -- (CenterUpper);}
\def\RightCenterUpperDiagonal{(BottomLeft) -- (CenterUpper);}


% - Diagonal strokes - bottomleft to CUR and BR to CUL

\def\BLCUR{(BottomLeft) -- (CenterUpperRight);}
\def\BRCUL{(BottomRight) -- (CenterUpperLeft);}


% - Arches
\def\ArchBottomLeft{(CenterUpperLeft) .. controls (CenterUpper) .. (CenterCellar);}
\def\ArchBottomRight{(CenterUpperRight) .. controls (CenterUpper) ..  (CenterCellar);}
\def\ArchUpperLeft{(UpperLeft).. controls (CenterCeiling) .. (Center);}
\def\ArchUpperRight{(UpperRight) .. controls (CenterCeiling) .. (Center);}
% - Ramps
\def\RampBottomLeft{(BottomLeft) .. controls (CenterCellar) ..  (CenterUpper);}
\def\RampBottomRight{(BottomRight) .. controls (CenterCellar) ..   (CenterUpper);}
\def\RampUpperLeft{(CenterLeft) .. controls (Center) ..   (CenterCeiling);}
\def\RampUpperRight{(CenterRight) .. controls (Center) ..   (CenterCeiling);}

% - Curves

\def\BigCurveUpperLeft{(CenterLeft) .. controls (Center) .. (UpperRight);}
\def\BigCurveUpperRight{(CenterRight) .. controls (Center) .. (UpperLeft);}


\def\CurveUpperLeft{(CenterCeiling) .. controls (Center) .. (CenterLeft);}
\def\CurveUpperRight{(CenterCeiling) .. controls (Center) .. (CenterRight);}
\def\CurveBottomLeft{(CenterCellar) .. controls (Center) .. (CenterLeft);}
\def\CurveUpperRight{(CenterCellar) .. controls (Center) .. (CenterRight);}

\def\BigCurveBottomLeft{(BottomLeft) .. controls (BottomRight) .. (CenterRight);}
\def\BigCurveBottomRight{(BottomRight) .. controls (BottomLeft) .. (CenterLeft);}

\def\LeftCurveBottomLeft{(BottomLeft) .. controls (CenterCellar) .. (Center);}
\def\LeftCurveBottomRight{(BottomRight) .. controls (CenterCellar) .. Center);}

\def\RightCurveBottomLeft{(Center) .. controls (CenterBottomRight) .. (CenterRight);}
\def\RightCurveBottomRight{(Center) .. controls (CenterBottomLeft) .. (CenterLeft);}


\def\BigCurveUpperRight{(CenterUpperLeft) .. controls (CenterCeiling) .. (UpperRight);}
\def\LeftCurveUpperRight{(CenterUpperLeft) .. controls (UpperLeft) .. (CenterUpper);}


\def\CurveTemp{(UpperLeft) .. controls (CenterBottomLeft) .. (CenterBottomRight);}

\def\CurveLR{(BottomLeft) .. controls (UpperLeft) .. (CenterUpperRight);}
\def\CurveRL{(BottomRight) .. controls (UpperRight) .. (CenterUpperLeft);}


%% Dot and Torus

%Dot
\def\Dot#1{(#1) circle (\thickness){};}

%Torus

\def\Torus#1{\draw[color=black, line width = 6 * \thickness] (#1) circle (\thickness){};}

%Cup

%\def\Cup#1{($(#1) +(-2,4)$) .. controls ($(#1) + (0,2)$) .. ($(#1)+(2,4)$); }


%\def\Cap#1{($(#1) +(2,-2)$) .. controls (#1) .. ($(#1)+(-2,-2)$); }
%\def\CapLeft#1{($(#1) +(2.6,-1)$) .. controls (#1) .. ($(#1)+(-1,-2.6)$); }
%\def\CapRight#1{($(#1) +(1,-2.6)$) .. controls (#1) .. ($(#1)+(-2.6,-1)$); }
%\def\Cap#1{($(#1) +(3,-1.5)$) .. controls (#1) .. ($(#1)+(-3,-1.5)$); }
%\def\CapLeft#1{($(#1) +(3,0.5)$) .. controls ($(#1) + (0,0.5) $) .. ($(#1)+(-3,-0.5)$); }
%\def\CapRight#1{($(#1) +(-3,0.5)$) .. controls ($(#1) + (0,0.5)$) .. ($(#1)+(3,-0.5)$); }



\def\BigCup#1{[black] ($(#1) + (-3, 0)$) to [bend left =-50] ($(#1) + (3,0)$);}
\def\BigCupLeft#1{[black] ($(#1) + (-3, -0.5)$) to [bend left =-50] ($(#1) + (3,1.5)$);}
\def\BigCupRight#1{[black] ($(#1) + (-3, 1.5)$) to [bend left =-50] ($(#1) + (3,-0.5)$);}

\def\BigCap#1{[black] ($(#1) + (-3, 0)$) to [bend left =50] ($(#1) + (3,0)$);}
\def\BigCapRight#1{[black] ($(#1) + (-3, -0.5)$) to [bend left =50] ($(#1) + (3,1.5)$);}
\def\BigCapLeft#1{[black] ($(#1) + (-3, 1.5)$) to [bend left =50] ($(#1) + (3,-0.5)$);}

\def\RightCap#1{[black] (#1)  to [bend left =50] ($(#1) + (3,0)$);}

\def\LeftCap#1{[black] (#1)  to [bend left =-50] ($(#1) + (-3,0)$);}

\def\RightCapLeft#1{[black] (#1)  to [bend left =50] ($(#1) + (3,-0.5)$);}
\def\LeftCapLeft#1{[black] (#1)  to [bend left =-50] ($(#1) + (-3,1.5)$);}
\def\LeftCapRight#1{[black] (#1)  to [bend left =-50] ($(#1) + (-3,-1.5)$);}
\def\RightCapRight#1{[black] (#1)  to [bend left =50] ($(#1) + (3,0.5)$);}

\def\RightCupLeft#1{[black] (#1)  to [bend left =-50] ($(#1) + (3,-0.5)$);}
\def\LeftCupLeft#1{[black] (#1)  to [bend left =50] ($(#1) + (-3,1.5)$);}
\def\LeftCupRight#1{[black] (#1)  to [bend left =50] ($(#1) + (-3,-1.5)$);}
\def\RightCupRight#1{[black] (#1)  to [bend left =-50] ($(#1) + (3,0.5)$);}

\def\RightCup#1{[black] (#1)  to [bend left =-50] ($(#1) + (3,0)$);}

\def\LeftCup#1{[black] (#1)  to [bend left =50] ($(#1) + (-3,0)$);}


\def\BigCircle{[black] (Center) circle (3){};}

\def\VS#1{[black, line width = \thickness] ($(#1) + (0,0.25)$) to ($(#1) + (0,-0.25)$);}
\def\HS#1{[black, line width = \thickness] ($(#1) + (0.25,0)$) to ($(#1) + (-0.25,0)$);}

\def\MediumCircle#1{(#1) circle (1.75){};}

%\def\Cup#1{($(#1) +(-2,2)$) .. controls (#1) .. ($(#1)+(2,2)$); }
%\def\CupLeft#1{($(#1) +(-2.6,1)$) .. controls (#1) .. ($(#1)+(1,2.6)$); }
%\def\CupRight#1{($(#1) +(-1,2.6)$) .. controls (#1) .. ($(#1)+(2.6,1)$); }



%\def\Cup#1{((#1) ++ (-2,2)) .. controls (#1) .. ((#1)++(2,2)) }

\def\SEL#1{($(#1) + (0.25,0)$) to [bend left = 55] ($(#1) + (3,3)$)}
\def\NWL#1{($(#1) + (0.25,0)$) to [bend left = -55] ($(#1) + (3,3)$)}
\def\SWL#1{($(#1) + (0.25,0)$) to [bend left = 55] ($(#1) + (3,-3)$)}
\def\NEL#1{($(#1) + (0.25,0)$) to [bend left = -55] ($(#1) + (3,-3)$)}
\def\NER#1{($(#1) + (0.25,0)$) to [bend left = 55] ($(#1) + (-3,3)$)}
\def\SER#1{($(#1) + (0.25,0)$) to [bend left = -55] ($(#1) + (-3,-3)$)}
\def\SWR#1{($(#1) + (0.25,0)$) to [bend left = -55] ($(#1) + (-3,3)$)}
\def\NWR#1{($(#1) + (0.25,0)$) to [bend left = 55] ($(#1) + (-3,-3)$)}
\def\NEC#1{($($(#1) + (0.85,0.85)$) + (-1.5,1.5)$) to [bend left = -55] ($($(#1) + (0.85,0.85)$) + (1.5,-1.5)$)}
\def\SEC#1{($($(#1) + (0.85,-0.85)$) + (-1.5,-1.5)$) to [bend left = 55] ($($(#1) + (0.85,-0.85)$) + (1.5,1.5)$)}
\def\SWC#1{($($(#1) + (-0.6,-0.6)$) + (-1.5,1.5)$) to [bend left = 55] ($($(#1) + (-0.6,-0.6)$) + (1.5,-1.5)$)}
\def\NWC#1{($($(#1) + (-0.6,0.6)$) + (-1.5,-1.5)$) to [bend left = -55] ($($(#1) + (-0.6,0.6)$) + (1.5,1.5)$)}
